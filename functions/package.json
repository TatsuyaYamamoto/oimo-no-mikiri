    "http-status-codes": "^1.3.0",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.11.1",
    "@types/morgan": "^1.7.35",
    "tslint": "^5.8.0",
    "typescript": "^2.5.3"
  },
