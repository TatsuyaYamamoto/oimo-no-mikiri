{
  "name": "oimo-no-mikiri",
  "version": "1.0.1",
  "description": "LoveLive fun game! Outclass an oimo.",
  "main": "index.js",
  "scripts": {
    "start": "run-p start:* serve:functions",
    "start:client": "webpack-dev-server --progress --colors --watch",
    "start:functions": "webpack --config webpack.config.functions.js --progress --colors --watch",
    "serve:functions": "firebase serve --only functions",
    "minify:image": "imagemin dist/assets/image/*.png --out-dir=dist/assets/image",
    "build": "run-p build:*",
    "build:client": "rimraf dist && webpack",
    "build:functions": "rimraf functions/index.functions.js && webpack --config webpack.config.functions.js --progress --colors",
    "build-production": "rimraf dist && NODE_ENV=production webpack && npm run minify:image",
    "shell": "npm run build && firebase experimental:functions:shell",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:dev": "node scripts/deploy.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TatsuyaYamamoto/oimo-no-mikiri.git"
  },
  "sFactory": {
    "deployPath": "oimo"
  },
  "keywords": [
    "lovelive",
    "oimo"
  ],
  "author": "Tatsuya Yamamoto",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TatsuyaYamamoto/oimo-no-mikiri/issues"
  },
  "homepage": "https://github.com/TatsuyaYamamoto/oimo-no-mikiri#readme",
  "devDependencies": {
    "@types/google.analytics": "^0.0.36",
    "@types/i18next": "^8.4.2",
    "@types/i18next-browser-languagedetector": "^2.0.0",
    "@types/mousetrap": "^1.5.34",
    "@types/node": "^8.0.28",
    "@types/pixi.js": "^4.5.3",
    "@types/query-string": "^5.0.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.7",
    "html-webpack-plugin": "^2.30.1",
    "imagemin-cli": "^3.0.0",
    "ora": "^1.3.0",
    "pug": "^2.0.0-rc.3",
    "pug-loader": "^2.3.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.3.3",
    "typescript": "^2.4.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.9",
    "vinyl-fs": "^2.4.4",
    "vinyl-ftp": "^0.6.0",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "animejs": "^2.0.2",
    "cors": "^2.8.4",
    "firebase": "^4.8.0",
    "firebase-admin": "^5.8.2",
    "firebase-functions": "^0.8.1",
    "http-status-codes": "^1.3.0",
    "i18next": "^9.0.0",
    "i18next-browser-languagedetector": "^2.0.0",
    "morgan": "^1.9.0",
    "mousetrap": "^1.6.1",
    "npm-run-all": "^4.1.2",
    "pixi-sound": "^1.4.2",
    "pixi.js": "^4.5.4",
    "query-string": "^5.1.0",
    "sweetalert": "^2.0.3",
    "tingle.js": "^0.13.0",
    "whatwg-fetch": "^2.0.3"
  }
}
